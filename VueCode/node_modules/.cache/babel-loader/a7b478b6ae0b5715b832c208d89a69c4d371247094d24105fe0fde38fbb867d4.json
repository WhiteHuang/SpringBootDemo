s} from \"@/router\";\r\n\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      user: {\r\n        username:\"monkey\",\r\n        password:\"123456\"\r\n      },\r\n      rules: {\r\n        username: [\r\n          {required: true, message: '请输入用户名', trigger: 'blur'},\r\n          {min: 3, message: \"长度不小于3个字符\", trigger: 'blur'}\r\n        ], password: [\r\n          {required: true, message: '请输入密码', trigger: 'blur'},\r\n          {min: 6, message: \"长度不小于6个字符\", trigger: 'blur'}\r\n        ]\r\n      }\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    login() {\r\n      this.$refs[\"userForm\"].validate((valid) => {\r\n            if(valid){\r\n              console.log(\"login\")\r\n              this.request.post(\"/user/login\", this.user).then(res => {\r\n                console.log(\"数据\")\r\n                console.log(res)\r\n                if (res.code === '200') {\r\n                  console.log(\"成功\")\r\n                  localStorage.setItem(\"user\",JSON.stringify(res.data))\r\n                  setRouters()\r\n                  this.$router.push(\"/\")\r\n                  this.$message.success(\"登录成功\")\r\n                } else {\r\n                  console.log(\"失败\")\r\n                  this.$message.error(res.msg)\r\n                }\r\n              })\r\n            }\r\n          }\r\n      );\r\n\r\n    },\r\n\r\n    register() {\r\nthis.$router.push(\"/register\")\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div class=\"wrapper\">\r\n    <div\r\n        style=\"margin: 15% auto;background-color:#fff;width: 350px;height: 300px;padding: 20px;border-radius: 10px;box-shadow: gray\">\r\n      <div style=\"margin: 20px 0;text-align: center;font-size: 24px\"><b>登录</b></div>\r\n      <el-form :model=\"user\" :rules=\"rules\" ref=\"userForm\">\r\n        <el-form-item prop=\"username\">\r\n          <el-input size=\"medium\" style=\"margin: 10px 0\" prefix-icon=\"el-icon-user\" v-model=\"user.username\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"password\">\r\n          <el-input size=\"medium\" style=\"margin: 10px 0\" prefix-icon=\"el-icon-lock\" show-password\r\n                    v-model=\"user.password\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item style=\"margin: 10px  0;text-align: center\">\r\n          <el-button type=\"primary\" size=\"medium\" autocomplete=\"off\" style=\"width: 40%\" @click=\"login\">登录</el-button>\r\n          <el-button type=\"warning\" size=\"medium\" autocomplete=\"off\" style=\"width: 40%\" @click=\"register\">注册\r\n          </el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.wrapper {\r\n  height: 100vh;\r\n  background-image: linear-gradient(to bottom left, #42b983, #3f5efb);\r\n  overflow: hidden;\r\n}\r\n</style>"],"mappings":";AACA,OAAAA,IAAA;AACA,SAAAC,UAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAH,IAAA;QACAI,QAAA;QACAC,QAAA;MACA;MACAC,KAAA;QACAF,QAAA,GACA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,GAAA;UAAAF,OAAA;UAAAC,OAAA;QAAA,EACA;QAAAJ,QAAA,GACA;UAAAE,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,GAAA;UAAAF,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EAEAE,OAAA;IACAC,MAAA;MACA,KAAAC,KAAA,aAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACAC,OAAA,CAAAC,GAAA;UACA,KAAAC,OAAA,CAAAC,IAAA,qBAAAnB,IAAA,EAAAoB,IAAA,CAAAC,GAAA;YACAL,OAAA,CAAAC,GAAA;YACAD,OAAA,CAAAC,GAAA,CAAAI,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cACAN,OAAA,CAAAC,GAAA;cACAM,YAAA,CAAAC,OAAA,SAAAC,IAAA,CAAAC,SAAA,CAAAL,GAAA,CAAAlB,IAAA;cACAF,UAAA;cACA,KAAA0B,OAAA,CAAAC,IAAA;cACA,KAAAC,QAAA,CAAAC,OAAA;YACA;cACAd,OAAA,CAAAC,GAAA;cACA,KAAAY,QAAA,CAAAE,KAAA,CAAAV,GAAA,CAAAW,GAAA;YACA;UACA;QACA;MACA,CACA;IAEA;IAEAC,SAAA;MACA,KAAAN,OAAA,CAAAC,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}